<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="cta_txt_state">
        <item name="android:textColor">@drawable/cta_txt_state</item>
    </style>

    <style name="home_section_title">
        <item name="android:fontFamily">@font/font_bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black_85</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="home_section_view_all">
        <item name="android:fontFamily">@font/font_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:alpha">0.19</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>
</resources>