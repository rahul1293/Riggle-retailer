<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="check"
            type="Boolean" />

        <variable
            name="viewModel"
            type="com.riggle.ui.profile.editprofile.EditProfileViewModel" />
    </data>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include layout="@layout/layout_toolbar" />

        <androidx.core.widget.NestedScrollView
            android:id="@+id/nestedScrollView"
            android:layout_width="match_parent"

            android:layout_height="match_parent"
            android:layout_below="@id/tabFrameLayout"
            android:clipToPadding="false"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvAboutYourStoreHeader"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:fontFamily="@font/font_bold"
                    android:lineSpacingExtra="3sp"
                    android:paddingTop="24dp"
                    android:text="@string/about_your_store"
                    android:textAllCaps="true"
                    android:textColor="@color/black_54"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <!--customBackground="@{check}"-->
                    <EditText
                        android:id="@+id/etStoreName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:background="@drawable/rounded_4dp_border"
                        android:fontFamily="@font/montserrat_medium"
                        android:hint="@string/store_name"
                        android:inputType="textPersonName|textNoSuggestions"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="16dp"
                        android:paddingTop="16dp"
                        android:paddingBottom="8dp"
                        android:text="@={viewModel.userData.retailer.name}"
                        android:textColor="@color/black_54"
                        android:textSize="16sp"
                        android:textStyle="normal"
                        tools:text="Akash Kiryana Store" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="-8dp"
                        android:background="@color/white"
                        android:fontFamily="@font/font_regular"
                        android:lineSpacingExtra="3sp"
                        android:padding="8dp"
                        android:text="Store Name"
                        android:textColor="@color/black_54"
                        android:textSize="12sp"
                        android:textStyle="normal" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="28dp"
                        android:layout_marginEnd="32dp"
                        android:onClick="onFieldLockClicked"
                        android:src="@drawable/ic_baseline_lock_24"
                        android:visibility="@{viewModel.userData.lockedKeys.mobile? View.VISIBLE: View.GONE, default = gone}"
                        app:tint="@color/black_38"
                        tools:visibility="visible" />
                </FrameLayout>

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/rounded_4dp_border"
                    android:fontFamily="@font/montserrat_medium"
                    android:hint="@string/owner_name"
                    android:inputType="textPersonName|textNoSuggestions"
                    android:lineSpacingExtra="3sp"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="12dp"
                    android:text="@={viewModel.userData.name}"
                    android:textColor="@color/black_85"
                    android:textSize="16sp"
                    android:textStyle="normal"
                    tools:text="Owner Name" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">

                    <EditText
                        android:id="@+id/etStoreType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:background="@drawable/rounded_4dp_border"
                        android:fontFamily="@font/montserrat_medium"
                        android:hint="@string/store_type"
                        android:inputType="textPersonName|textNoSuggestions"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="16dp"
                        android:paddingTop="16dp"
                        android:paddingBottom="8dp"
                        android:text="@={viewModel.userData.retailer.store_type}"
                        android:textColor="@color/black_85"
                        android:textSize="16sp"
                        android:textStyle="normal"
                        tools:text="Akash Kiryana Store" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="-8dp"
                        android:background="@color/white"
                        android:fontFamily="@font/font_regular"
                        android:lineSpacingExtra="3sp"
                        android:padding="8dp"
                        android:text="@string/store_type"
                        android:textColor="@color/black_54"
                        android:textSize="12sp"
                        android:textStyle="normal" />
                </FrameLayout>


                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">

                    <EditText
                        customBackground='@{check}'
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="16dp"
                        android:fontFamily="@font/montserrat_medium"
                        android:inputType="phone"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="16dp"
                        android:paddingTop="16dp"
                        android:paddingBottom="8dp"
                        android:text="@{viewModel.userData.mobile}"
                        android:textColor="@color/black_54"
                        android:textSize="16sp"
                        android:textStyle="normal"
                        tools:background="@drawable/rounded_4dp_border_16padding"
                        tools:text="123456789" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="-12dp"
                        android:background="@color/white"
                        android:fontFamily="@font/font_regular"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="8dp"
                        android:paddingVertical="2dp"
                        android:text="@string/Phone_Number"
                        android:textColor="@color/black_54"
                        android:textSize="12sp"
                        android:textStyle="normal" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="28dp"
                        android:layout_marginEnd="32dp"
                        android:onClick="onFieldLockClicked"
                        android:src="@drawable/ic_baseline_lock_24"
                        android:visibility="@{viewModel.userData.lockedKeys.mobile? View.VISIBLE: View.GONE, default = gone}"
                        app:tint="@color/black_38"
                        tools:visibility="visible" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:paddingBottom="24dp"
                    android:visibility="gone">

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:background="@drawable/rounded_4dp_border"
                        android:fontFamily="@font/montserrat_medium"
                        android:inputType="textPersonName|textNoSuggestions"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="16dp"
                        android:paddingTop="16dp"
                        android:paddingBottom="8dp"
                        android:text="@={viewModel.userData.alternate_no}"
                        android:textColor="@color/black_85"
                        android:textSize="16sp"
                        android:textStyle="normal"
                        tools:text="Akash Kiryana Store" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="-8dp"
                        android:background="@color/white"
                        android:fontFamily="@font/font_regular"
                        android:lineSpacingExtra="3sp"
                        android:padding="8dp"
                        android:text="@string/alternate_number"
                        android:textColor="@color/black_54"
                        android:textSize="12sp"
                        android:textStyle="normal" />
                </FrameLayout>

                <TextView
                    android:id="@+id/tvAddressHeader"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:fontFamily="@font/font_bold"
                    android:lineSpacingExtra="3sp"
                    android:paddingTop="24dp"
                    android:paddingBottom="16dp"
                    android:text="@string/address"
                    android:textAllCaps="true"
                    android:textColor="@color/black_54"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:visibility="gone" />


                <TextView
                    android:id="@+id/tvAdditionalInfoHeader"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:fontFamily="@font/font_bold"
                    android:lineSpacingExtra="3sp"
                    android:paddingTop="16dp"
                    android:text="@string/additional_info"
                    android:textAllCaps="true"
                    android:textColor="@color/black_54"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">

                    <EditText
                        android:id="@+id/etAddress"
                        customBackground="@{check}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="16dp"
                        android:fontFamily="@font/montserrat_medium"
                        android:gravity="top"
                        android:lineSpacingExtra="3sp"
                        android:minHeight="108dp"
                        android:paddingHorizontal="16dp"
                        android:paddingTop="16dp"
                        android:paddingBottom="8dp"
                        android:text="@={viewModel.userData.retailer.address}"
                        android:textColor="@color/black_54"
                        android:textSize="16sp"
                        android:textStyle="normal"
                        tools:background="@drawable/rounded_4dp_border_16padding"
                        tools:text="number" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="-8dp"
                        android:background="@color/white"
                        android:fontFamily="@font/font_regular"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="8dp"
                        android:paddingVertical="2dp"
                        android:text="@string/address"
                        android:textColor="@color/black_54"
                        android:textSize="12sp"
                        android:textStyle="normal" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="28dp"
                        android:layout_marginEnd="32dp"
                        android:onClick="onFieldLockClicked"
                        android:src="@drawable/ic_baseline_lock_24"
                        android:visibility="@{viewModel.userData.lockedKeys.address? View.VISIBLE: View.GONE, default = gone}"
                        app:tint="@color/black_38"
                        tools:visibility="visible" />
                </FrameLayout>


                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">

                    <EditText
                        android:id="@+id/etPincode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="16dp"
                        android:fontFamily="@font/montserrat_medium"
                        android:inputType="textPersonName|textNoSuggestions"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="16dp"
                        android:paddingTop="16dp"
                        android:background="@drawable/rounded_4dp_border"
                        android:paddingBottom="8dp"
                        android:text="@={viewModel.retailerData.pincode}"
                        android:textColor="@color/black_54"
                        android:textSize="16sp"
                        android:textStyle="normal"
                        tools:background="@drawable/rounded_4dp_border_16padding"
                        tools:text="144008" />
                    <!--customBackground="@{check}"-->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="-8dp"
                        android:background="@color/white"
                        android:fontFamily="@font/font_regular"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="8dp"
                        android:paddingVertical="2dp"
                        android:text="@string/pin_code"
                        android:textColor="@color/black_54"
                        android:textSize="12sp"
                        android:textStyle="normal" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="28dp"
                        android:layout_marginEnd="32dp"
                        android:onClick="onFieldLockClicked"
                        android:src="@drawable/ic_baseline_lock_24"
                        android:visibility="@{viewModel.userData.lockedKeys.pincode? View.VISIBLE: View.GONE, default = gone}"
                        app:tint="@color/black_38"
                        tools:visibility="visible" />
                </FrameLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:orientation="horizontal">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/etArea"
                            customBackground="@{viewModel.userData.lockedKeys.city}"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginEnd="8dp"
                            android:focusable="false"
                            android:focusableInTouchMode="false"
                            android:fontFamily="@font/montserrat_medium"
                            android:hint="@string/area"
                            android:inputType="textPersonName|textNoSuggestions"
                            android:lineSpacingExtra="3sp"
                            android:paddingHorizontal="16dp"
                            android:paddingVertical="12dp"
                            android:text="@={viewModel.retailerData.sub_area.belongs.name}"
                            android:textColor="@color/black_54"
                            android:textSize="16sp"
                            android:textStyle="normal"
                            tools:background="@drawable/rounded_4dp_border_16padding"
                            tools:text="@string/city" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:layout_marginTop="28dp"
                            android:layout_marginEnd="24dp"
                            android:onClick="onFieldLockClicked"
                            android:src="@drawable/ic_baseline_lock_24"
                            android:visibility="@{viewModel.userData.lockedKeys.city? View.VISIBLE: View.GONE, default = gone}"
                            app:tint="@color/black_38"
                            tools:visibility="visible" />
                    </FrameLayout>


                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/etSubArea"
                            customBackground="@{viewModel.userData.lockedKeys.state}"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginEnd="16dp"
                            android:focusable="false"
                            android:focusableInTouchMode="false"
                            android:fontFamily="@font/montserrat_medium"
                            android:hint="@string/sub_area"
                            android:inputType="textPersonName|textNoSuggestions"
                            android:lineSpacingExtra="3sp"
                            android:paddingHorizontal="16dp"
                            android:paddingVertical="12dp"
                            android:text='@={viewModel.retailerData.sub_area.name}'
                            android:textColor="@color/black_54"
                            android:textSize="16sp"
                            android:textStyle="normal"
                            tools:background="@drawable/rounded_4dp_border_16padding"
                            tools:text="State" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:layout_marginTop="28dp"
                            android:layout_marginEnd="32dp"
                            android:onClick="onFieldLockClicked"
                            android:src="@drawable/ic_baseline_lock_24"
                            android:visibility="@{viewModel.userData.lockedKeys.state? View.VISIBLE: View.GONE, default = gone}"
                            app:tint="@color/black_38"
                            tools:visibility="visible" />
                    </FrameLayout>


                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rvSubArea"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/llArea"
                    android:nestedScrollingEnabled="false"
                    android:visibility="gone" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">

                    <EditText
                        android:id="@+id/etGstnNumber"
                        customBackground="@{viewModel.userData.lockedKeys.gstn_no}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="16dp"
                        android:fontFamily="@font/montserrat_medium"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="16dp"
                        android:paddingTop="16dp"
                        android:paddingBottom="8dp"
                        android:text="@={viewModel.userData.gstn_no}"
                        android:textColor="@color/black_54"
                        android:textSize="16sp"
                        android:textStyle="normal"
                        tools:background="@drawable/rounded_4dp_border_16padding"
                        tools:text="asdasdas=123123" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="-8dp"
                        android:background="@color/white"
                        android:fontFamily="@font/font_regular"
                        android:lineSpacingExtra="3sp"
                        android:paddingHorizontal="8dp"
                        android:paddingVertical="2dp"
                        android:text="@string/gstn_number"
                        android:textColor="@color/black_54"
                        android:textSize="12sp"
                        android:textStyle="normal" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="28dp"
                        android:layout_marginEnd="32dp"
                        android:onClick="onFieldLockClicked"
                        android:src="@drawable/ic_baseline_lock_24"
                        android:visibility="@{viewModel.userData.lockedKeys.gstn_no? View.VISIBLE: View.GONE, default = gone}"
                        app:tint="@color/black_38"
                        tools:visibility="visible" />
                </FrameLayout>

                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/cbAtStore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:buttonTint="@color/black_54"
                    android:drawablePadding="6dp"
                    android:fontFamily="@font/font_medium"
                    android:text="Are you at store?"
                    android:textColor="@color/black_85"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tvUploadDocumentsHeader"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:fontFamily="@font/font_regular"
                    android:lineSpacingExtra="4sp"
                    android:text="Upload documents"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    android:visibility="gone"
                    android:textStyle="normal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/uploadPhotoTextView"
                        android:layout_width="84dp"
                        android:layout_height="84dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="96dp"
                        android:layout_weight="0"
                        android:background="@drawable/shape_dotted_border"
                        android:drawableTop="@drawable/ic_plus"
                        android:drawablePadding="16dp"
                        android:drawableTint="@color/colorPrimary"
                        android:fontFamily="@font/font_regular"
                        android:gravity="center_horizontal"
                        android:paddingTop="20dp"
                        android:text="@string/upload"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary" />

                    <!--
                                        <androidx.recyclerview.widget.RecyclerView
                                            android:id="@+id/addPostImagesRecyclerView"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginBottom="8dp"
                                            android:layout_weight="1"
                                            android:clipToPadding="false"
                                            android:orientation="horizontal"
                                            android:paddingStart="16dp"
                                            android:paddingEnd="16dp"
                                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                            app:layout_constraintEnd_toEndOf="parent"
                                            app:layout_constraintStart_toStartOf="parent"
                                            app:layout_constraintTop_toTopOf="parent"
                                            app:uriList="@{viewModel.imagesFileArrayList}" />-->

                    <ImageView
                        android:id="@+id/documentImageView"
                        url="@{viewModel.documentUrl}"
                        android:layout_width="84dp"
                        android:layout_height="84dp"
                        android:layout_marginStart="16dp" />
                </LinearLayout>


                <TextView
                    android:id="@+id/tvSave"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/colorPrimary"
                    android:clickable="true"
                    android:focusable="true"
                    android:fontFamily="@font/font_bold"
                    android:foreground="?attr/selectableItemBackground"
                    android:gravity="center"
                    android:paddingTop="16dp"
                    android:paddingBottom="16dp"
                    android:text="@string/save"
                    android:textAllCaps="true"
                    android:textColor="@color/white"
                    android:visibility="visible" />
            </LinearLayout>

        </androidx.core.widget.NestedScrollView>


        <FrameLayout
            android:id="@+id/tabFrameLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/toolbar">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.google.android.material.tabs.TabLayout
                    android:id="@+id/tabLayout"
                    android:layout_width="match_parent"
                    android:layout_height="?android:attr/actionBarSize"
                    android:background="@android:color/white"
                    app:layout_constraintTop_toTopOf="parent"
                    app:tabIndicator="@drawable/edit_profile_tab_indicator"
                    app:tabIndicatorColor="@color/colorPrimary"
                    app:tabSelectedTextColor="@color/colorPrimary"
                    app:tabTextAppearance="@style/TabStyle"
                    app:tabTextColor="@color/black_54" />


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/seperator_clr" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="?android:attr/actionBarSize"
                android:orientation="horizontal">

                <View
                    android:id="@+id/aboutStoreClickableView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    tools:background="@drawable/btn_rounded_corner_10dp" />

                <!-- <View
                     android:id="@+id/addressClickableView"
                     android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:layout_weight="1"
                     tools:background="@drawable/btn_rounded_corner_10dp" />-->

                <View
                    android:id="@+id/additionalInfoClickableView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    tools:background="@drawable/btn_rounded_corner_10dp" />

            </LinearLayout>


        </FrameLayout>

    </RelativeLayout>
</layout>